{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto",
        "singleQuote": true
      }
    ],
    "no-extend-native": ["error", { "exceptions": ["Number"] }]
  },
  "globals": {
    "window": "writable",
    "it": "readonly",
    "describe": "readonly",
    "expect": "readonly"
  }
}
